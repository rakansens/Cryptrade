{
  "name": "cryptrade",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit --pretty",
    "type-check:dev": "tsc --project tsconfig.dev.json --noEmit --pretty",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:ci": "tsc --project tsconfig.ci.json --noEmit --pretty",
    "type-check:prod": "tsc --project tsconfig.prod.json --noEmit",
    "type-coverage": "type-coverage --detail --show-relative-path",
    "ci:validate": "npm run lint && npm run type-check:ci && npm run test",
    "ci:full": "npm run lint && npm run type-check:ci && npm run type-coverage && npm run test:coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "tsx app/api/__tests__/run-api-tests.ts",
    "test:api:watch": "jest --watch app/api",
    "test:components": "jest components",
    "test:components:watch": "jest --watch components",
    "test:hooks": "jest hooks",
    "test:hooks:watch": "jest --watch hooks",
    "test:lib": "jest lib",
    "test:lib:watch": "jest --watch lib",
    "test:store": "jest store",
    "test:store:watch": "jest --watch store",
    "test:types": "jest types",
    "test:types:watch": "jest --watch types",
    "test:config": "jest config",
    "test:config:watch": "jest --watch config",
    "test:analysis": "jest lib/analysis",
    "test:analysis:watch": "jest --watch lib/analysis",
    "test:ws": "jest lib/ws",
    "test:ws:watch": "jest --watch lib/ws",
    "test:mastra": "jest lib/mastra",
    "test:mastra:watch": "jest --watch lib/mastra",
    "test:indicators": "jest lib/indicators",
    "test:indicators:watch": "jest --watch lib/indicators",
    "test:utils": "jest lib/utils",
    "test:utils:watch": "jest --watch lib/utils",
    "test:chat": "jest components/chat",
    "test:chat:watch": "jest --watch components/chat",
    "test:chart": "jest components/chart",
    "test:chart:watch": "jest --watch components/chart",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:unit:watch": "jest --watch --testPathIgnorePatterns=e2e",
    "test:integration": "jest __tests__/integration",
    "test:integration:watch": "jest --watch __tests__/integration",
    "test:integration:orchestrator": "jest __tests__/integration/orchestrator",
    "test:integration:proposals": "jest __tests__/integration/proposals", 
    "test:integration:api": "jest __tests__/integration/api",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:shard": "jest --shard",
    "test:errors": "jest lib/errors",
    "test:monitoring": "jest lib/monitoring",
    "test:api:unit": "jest app/api",
    "test:parallel": "jest --maxWorkers=50%",
    "test:coverage:html": "jest --coverage --coverageReporters=html",
    "test:coverage:lcov": "jest --coverage --coverageReporters=lcov",
    "test:changed": "jest --onlyChanged",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "test:memory-demo": "tsx scripts/test-memory-demo.ts",
    "test:memory-api": "tsx scripts/test-memory-api.ts",
    "test:performance": "jest __tests__/performance --testTimeout=60000",
    "benchmark": "tsx __tests__/performance/benchmark-runner.ts",
    "benchmark:compare": "tsx __tests__/performance/benchmark-runner.ts --compare",
    "benchmark:baseline": "tsx __tests__/performance/benchmark-runner.ts --save-baseline",
    "docs:api": "typedoc --out docs/api --entryPointStrategy expand --entryPoints lib --name \"Cryptrade API Documentation\" --skipErrorChecking",
    "env:check": "node scripts/env-validate.mjs",
    "prebuild": "npm run env:check",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "supabase db seed",
    "db:studio": "prisma studio",
    "db:setup": "npm run db:start && npm run db:push && npm run db:seed",
    "db:generate": "prisma generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@mastra/core": "^0.10.1",
    "@mastra/openai": "^1.0.1-alpha.48",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@vercel/kv": "^3.0.0",
    "blessed": "^0.1.81",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.1",
    "eventsource": "^4.0.0",
    "framer-motion": "^12.16.0",
    "immer": "^10.1.1",
    "isomorphic-dompurify": "^2.25.0",
    "lightweight-charts": "^4.1.3",
    "lucide-react": "^0.358.0",
    "next": "15.3.3",
    "next-themes": "^0.2.1",
    "openai": "^5.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^2.1.3",
    "rxjs": "^7.8.2",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.45",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@prisma/client": "^6.9.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "20.6.2",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "better-sqlite3": "^11.10.0",
    "chalk": "^5.4.1",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "msw": "^2.8.7",
    "node-fetch": "^3.3.2",
    "postcss": "8.4.30",
    "prisma": "^6.9.0",
    "puppeteer": "^24.10.0",
    "supabase": "^2.24.3",
    "tailwindcss": "3.3.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.4",
    "typedoc": "^0.28.5",
    "typescript": "5.2.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  }
}
